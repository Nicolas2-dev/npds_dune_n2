{
    "name": "nicolas/npds_dune_n2",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Npds\\": "npds/"
        },
        "files": [
            "storage/generated_helpers_autoload.php",
            "npds/Collections/helpers.php",
            "npds/Support/helpers.php"
        ]
    },
    "authors": [
        {
            "name": "Nicolas2-dev",
            "email": "nicolas.l.devoy@gmail.com"
        }
    ],
    "require-dev": {
        "filp/whoops": "^2.18",
        "symfony/var-dumper": "^7.3"
    },
    "require": {
        "phpmailer/phpmailer": "^6.10",
        "psr/container": "^2.0",
        "symfony/http-foundation": "^7.3",
        "nesbot/carbon": "^3.10",
        "symfony/http-kernel": "^7.3",
        "symfony/routing": "^7.3"
    },    
    "scripts": {
        "post-autoload-dump": [
            "php -r \"require __DIR__.'/npds/Support/HelperGenerator.php'; \\Npds\\Support\\HelperGenerator::generate();\""
        ]
    }
}
