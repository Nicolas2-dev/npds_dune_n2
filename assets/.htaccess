# Bloque tous les PHP sauf index.php, listskin.php et emojicontroller.php
<FilesMatch "\.(php|php[0-9]?|phtml)$">
    Require all denied
</FilesMatch>

<Files "index.php">
    Require all granted
</Files>

<Files "listskin.php">
    Require all granted
</Files>

<Files "emojicontroller.php">
    Require all granted
</Files>

<Files "formhelp.java.php">
    Require all granted
</Files>

Options -Indexes

ErrorDocument 403 /index.php
